{
  "name": "flannel",
  "version": "0.9.5",
  "description": "It's easy to handle logs when your coffee-script classes wear Flannel",
  "main": "lib/flannel.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.spec.coffee",
    "pretest": "./node_modules/.bin/coffee --bare --output ./lib --compile ./src/*.coffee"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doublerebel/flannel.git"
  },
  "keywords": [
    "log",
    "logger",
    "mixin",
    "coffeescript",
    "coffee-script",
    "couchdb",
    "logentries",
    "loggly",
    "logzio",
    "mongodb",
    "riak"
  ],
  "author": "doublerebel",
  "license": "MIT",
  "dependencies": {
    "split": "^1.0.0",
    "winston": "~2.2.0"
  },
  "optionalDependencies": {
    "morgan": "~1.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-errors": "^0.8.5",
    "coffee-script": "~1.10.0",
    "le_node": "^1.2.0",
    "mocha": "^2.4.5",
    "sinon": "^1.9.1",
    "sinon-chai": "^2.5.0",
    "winston-couchdb": "^0.6.3",
    "winston-loggly": "^1.3.1",
    "winston-logzio": "^1.0.0",
    "winston-mongodb": "^1.3.0",
    "winston-riak": "^0.3.2"
  }
